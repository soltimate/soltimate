{
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=TestSoltimate;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "Auth": {
    "IncludeScopes": false,
    "Password": {
      "RequireDigit": false,
      "RequireNonAlphanumeric": false,
      "RequireUppercase": false,
      "RequiredLength": 6,
      "RequiredUniqueChars": 3
    },
    "Lockout": {
      "DefaultLockoutMinutes": 30,
      "MaxFailedAccessAttempts": 10
    },
    "SignIn": {
      "RequireConfirmedEmail": false
    },
    "Cookie": {
      "Name": "SoltimateTest",
      "ExpirationDays": 14
    }
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  }
}
